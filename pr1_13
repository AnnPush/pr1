# include <stdio.h>

float isMinimum(float, float, float);

int main()
{
	int i1, i2, i3;
	printf("%s", "Enter three floating-point numbers:");
	scanf("%f%f%f", &i1, &i2, &i3);

	printf("%f", isMinimum(i1, i2, i3);
}

float isMinimum( int a, int b, int c)
{
	floai min;

	if(a < b)
		min = a;
	else
		min = b;
	
	if(c < min)
		min = c;
	return min;
}
